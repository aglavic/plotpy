<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="text/4circle">
     <comment>SPEC data written with 4circle diffractometer</comment>
     <magic priority="50">
       <match value="#F" type="string" offset="0"/>
     </magic>
     <glob pattern="*.spec"/>
     <glob pattern="*.spec.gz"/>
     <glob pattern="*.fio"/>
     <glob pattern="*.fio.gz"/>
   </mime-type>
</mime-info>
